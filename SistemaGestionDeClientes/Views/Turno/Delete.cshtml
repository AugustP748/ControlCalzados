@model Model.Domain.ControlDeCalzado.Turno

@{
    ViewBag.Title = "Turno";
}

<h2>Eliminar Turno</h2>

<h3>Estas seguro que quieres eliminar?</h3>
<div>
    @*<h4>Turno</h4>*@
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.HoraDeInicio)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.HoraDeInicio)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.HoraDeFin)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.HoraDeFin)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
    @Html.AntiForgeryToken()

    <div class="form-actions no-color">
        <input type="submit" value="Eliminar Turno" class="btn" /> |
        <a class="btn" href="javascript:void(0);" onclick="history.go(-1);">Volver a la Lista</a>
    </div>
    }
</div>
